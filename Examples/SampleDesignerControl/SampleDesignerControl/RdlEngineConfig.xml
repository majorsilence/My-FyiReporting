<config>
	<DataSources>
		<DataSource>
			<DataProvider>SQL</DataProvider>
			<TableSelect>SELECT TABLE_NAME, TABLE_TYPE FROM INFORMATION_SCHEMA.TABLES ORDER BY 2, 1</TableSelect>
			<Interface>SQL</Interface>
		</DataSource>
		<DataSource>
			<DataProvider>ODBC</DataProvider>
			<TableSelect>SELECT TABLE_NAME, TABLE_TYPE FROM INFORMATION_SCHEMA.TABLES ORDER BY 2, 1</TableSelect>
